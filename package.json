{
  "name": "layabout",
  "version": "0.7.3",
  "description": "Simple layout components for React",
  "author": "Dan Mutton",
  "license": "Apache-2.0",
  "homepage": "https://github.com/yearofthedan/layabout#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yearofthedan/layabout.git"
  },
  "bugs": {
    "url": "https://github.com/yearofthedan/layabout/issues"
  },
  "keywords": [
    "react",
    "layout"
  ],
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "yarn build",
    "build": "NODE_ENV=production babel src -d dist --ignore '**/*-test.js'",
    "lint": "eslint src",
    "test": "yarn lint && jest",
    "tdd": "jest --watch",
    "demo": "start-storybook -p 9009 -c demo",
    "build-demo-docs": "build-storybook -c demo -o docs"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.0.0",
    "@storybook/addon-knobs": "^3.0.0",
    "@storybook/react": "^3.0.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "17.0.2",
    "babel-preset-react-app": "^2.0.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "jest": "17.0.2",
    "jest-enzyme": "^2.1.0-rc2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.5.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setup-test.js"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
