{
  "name": "layabout",
  "version": "0.8.10",
  "description": "Simple layout components for React",
  "author": "Dan Mutton",
  "license": "Apache-2.0",
  "homepage": "https://github.com/yearofthedan/layabout#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yearofthedan/layabout.git"
  },
  "bugs": {
    "url": "https://github.com/yearofthedan/layabout/issues"
  },
  "keywords": [
    "react",
    "layout",
    "grid",
    "flexbox"
  ],
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "yarn build",
    "bump-minor": "npm version minor -m",
    "build": "NODE_ENV=production babel src -d dist --ignore '**/*.test.js'",
    "lint": "eslint src --fix",
    "test": "jest && yarn lint",
    "tdd": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "demo": "start-storybook",
    "build-demo-docs": "build-storybook -o docs"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-info": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.6.0",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.0.0",
    "jest-environment-enzyme": "^7.0.1",
    "jest-enzyme": "^7.0.1"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react-dom-factories": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "resetMocks": true
  }
}
